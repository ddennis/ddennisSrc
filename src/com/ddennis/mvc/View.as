package com.ddennis.mvc{	import flash.display.Sprite;	import flash.events.Event;		public class View extends Sprite	{				private var _model:*;				public function View()		{					}				public function update(event:Event = null):void	{					}						public function set model(m:*):void {						_model = m;			_model.addEventListener(Model.MODEL_CHANGE, update);					}				public function get model():*		{			return _model;		}	}}