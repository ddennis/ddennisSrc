package com.ddennis.mvc {		import com.ddennis.mvc.AppModel;	import flash.display.MovieClip;	import flash.events.Event;		public class AppView extends MovieClip	{				private var _model:AppModel;				public function AppView()		{					}				public function update(event:Event = null):void	{					}				public function dataLoaded(e:Event):void {					}						public function set model(m:AppModel):void {						_model = m;			_model.addEventListener(AppModel.MODEL_CHANGE, update);			_model.addEventListener(AppModel.DATA_LOADED, dataLoaded);					}									public function get model():AppModel		{			return _model;		}	}}